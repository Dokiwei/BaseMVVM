<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/app"
    app:startDestination="@id/mainNavFragment">

    <fragment
        android:id="@+id/mainNavFragment"
        android:name="com.dokiwei.basemvvm.ui.app.MainNavFragment"
        android:label="MainNavFragment" >
        <action
            android:id="@+id/action_mainNavFragment_to_customViewFragment"
            app:destination="@id/customViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/customViewFragment"
        android:name="com.dokiwei.basemvvm.ui.customview.CustomViewFragment"
        android:label="CustomViewFragment" >
        <action
            android:id="@+id/action_customViewFragment_to_mainNavFragment"
            app:destination="@id/mainNavFragment" />
    </fragment>
</navigation>